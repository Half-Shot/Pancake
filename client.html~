	<script>
		//Keep our program class here. Best shot we have ;)
		var modloader = new ModuleLoader();
		document.addEventListener("mod_finish",
			function(){
				var program = new Program();
				program.init();
			}
		, true);
		modloader.load();
	</script>
	<!-- Note: Don't add add any javascripts here. They can be loaded through the module loader in CLoader.js-->
